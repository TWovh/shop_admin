[run]
source = shop,shopadmin
omit = 
    */tests/*
    */migrations/*
    */venv/*
    */__pycache__/*
    manage.py
    */test_*.py

[report]
exclude_lines =
    pragma: no cover
    def __repr__
    raise AssertionError
    raise NotImplementedError
    if 0:
    if __name__ == .__main__.:
    class .*\bProtocol\):
    @(abc\.)?abstractmethod 